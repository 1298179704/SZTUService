/* pages/share/share.wxss */

page{
  width: 100%;
  height: 100%;
  background-color: #ffffff;
  background-image: url("data:image/png;base64,");

}

.Content {
  display: flex;
  flex-direction: column;
  position: relative;
  top: 70rpx;
  margin: 0 18rpx 40rpx 18rpx;
  background-color: #ffffff;
  border-radius: 20rpx;
  color: #aa9680;
  box-shadow: 0rpx 0rpx 10rpx 3rpx #e2e2e2;
}
.majority{
  display: flex;
  flex-direction: row;
  margin: 30rpx;
  align-items: center;
  bottom: 0rpx;
  background: #f8f8f8;
  border-radius: 10rpx;
  

}
.type{
  color: #5a5858;
  font-weight: lighter;
  margin: 10rpx;
  position: relative;
  left: 0rpx;
  display: flex;
  flex-direction: row
  

}
.type-content1{
  margin: 0 20rpx 0 0rpx;
}
.type-content2{
  margin: 0 0 0 20rpx;

}

.join-button {
  position: absolute;
  right: 40rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  color: #fff;
  background: #2e6e92;
  border: 0;
  font-size: small;
  width: 160rpx;
  height: 50rpx;
  border-radius: 40rpx;
  font-weight: lighter;
}


.main {

  margin: 50rpx 30rpx 20rpx 30rpx;
 
  position: relative;

  bottom: 10rpx;
  background: #fafafa;
  border-radius: 30rpx;
  padding: 20rpx;
  


}


.start-end {
 z-index: 10;
position: relative;
display: flex;
flex-direction: row;
width: max-content;
margin: -10rpx 10rpx ;
color: #5a5858;
background-color: #f8f8f8

}
.arrow{
   z-index: 10;
  position: relative;
  left:10%
}

.start-end-title {
position: relative;
display: flex;
flex-direction: row;
float: right;
align-items: center
}

.start-end-text {
position: relative;
width: 90%;





}
.detail-title{
  color: #ffffff;
   background:#ced3d4;
  border-radius: 8rpx;
  width: 80rpx;
  height: auto;
  font-size: x-small;
  font-weight: lighter;
  position: absolute;
  left: 10rpx;
  top: -3rpx;
  text-align: center;



}

.Txtcolor {
  color: #d48b1d;
  font-weight: normal;

}

.row {
  position: relative;
  display: flex;
  align-items: center;
  width: 100%;
  font-weight: lighter;
  border-bottom: 5rpx solid #f5f5f5;

  margin: 30rpx 0;

  
}

.gender {
  margin: 0 0 0 20rpx;
}



.note {
  position: relative;
  display: flex;
  flex-direction: row;
  background: rgb(250, 250, 250);
  border-radius: 30rpx;
  
  font-weight: lighter;

}

.note-title {
  position: relative;
  display: flex;
  flex-direction: row;
  align-items: center;
  
}

.note-text {
  position: relative;
  float: right;
  width: 66%;
  left: 15rpx;
  display: flex;
  align-items: center;
  
}

.horizon {
  display: flex;
  flex-direction: row;
}
.member-title{
  color: #ffffff;
   background:#d8b683;
  border-radius: 8rpx;
  width: max-content;
 
  font-weight: lighter;

  text-align: center;
}
.bnt-join{
  background-color: #0A6E92;
  color: #ffffff;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  position: relative;
  top: 60rpx;
  left: 4.6%;

  width: 45%;
  height: 90rpx;
  float: left;
  border-top-left-radius: 60rpx;
  border-bottom-left-radius: 60rpx;
 border-bottom-right-radius:0rpx;
  border-top-right-radius: 0rpx;
  border: 0;
  
}
.bnt-share{
  background-color: #eb7b1f;
  color: #ffffff;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  float: right;
  position: relative;
  top: 60rpx;
  right: 5%;
  width: 45%;
  height: 90rpx;
  border-top-left-radius: 0;
  border-bottom-left-radius: 0;
 border-bottom-right-radius: 60rpx;
  border-top-right-radius: 60rpx;
  border: 0;

}