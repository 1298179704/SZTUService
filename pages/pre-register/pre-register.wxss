
page{
  width: 100%;
  height: 100%;
  background-color: #0A6E92
}

.main{
  position: relative;
  top: 100rpx;
  margin:0 70rpx;
  border-radius: 25rpx;
  background: #ffffff;
  display: flex;


}

.place-holder{
  font-weight: lighter;
}
.title{
  position: relative;
  left: 5%;
  top: 10rpx;
  color: #777373;
  font-size: small;
  display: flex;
  align-items: center;
  justify-content: center;
  border-bottom: solid 4rpx #f5f5f5;
 margin: 0 50rpx;

  
}

.column{
  position: relative;
  margin: 30rpx 20rpx 20rpx 30rpx;
  top: 10px;
  color: #5c5959;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.img-title{
  margin: 0 20rpx
}
.input-style{
  display: flex;
  align-items: center;
  text-align: center;
  max-width: 65%;
  
  height: 70rpx;
  line-height: 70rpx;

 background: rgb(243, 243, 243);
 border-radius: 30rpx;

  margin: 0 0 0 20rpx ;

}

.submit-btn{
  position: relative;
  display: flex;
  right: 105rpx;
  top: 0rpx;
  float: right

}
.hover{
 opacity:0.3
}
.text1{
  position: relative;
  text-align: center;
  margin: 30rpx 20rpx;
  font-size: xx-small;
  font-weight: lighter;
  color: #cfcfcf;
  width: 60%;
  left: 20%;


}
.text2{
  position: absolute;
  left: 5%;
  bottom: 100rpx;
  font-size:x-small;
  font-weight: lighter;
  color: #ececec;
  width: 90%;
  text-align: center;
}
.service{
  font-size: small;
  position: absolute;
  z-index: 100;
  bottom: 10rpx;
  left: 20%;
  width: 60%;
  border-radius: 20rpx;
  background-color: #ffffff;
  font-weight: bold;
  color: #318aaa;
  display: flex;
  flex-direction: row;
  align-items: center;
 justify-content: center;


  
}